<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:sweet="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="1dp"/>
            <gradient android:startColor="@color/color_bar" android:endColor="@color/color_bar" android:angle="270"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="1dp"/>
                <gradient android:startColor="@color/crop_selected_color" android:endColor="@color/crop_selected_color" android:angle="270"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="1dp"/>
                <gradient android:startColor="@color/crop_selected_color" android:endColor="@color/crop_selected_color" android:angle="270"/>
            </shape>
        </clip>
    </item>
</layer-list>
